<%= form_for(@finish) do |f| %>
	<%= f.error_messages %>
	<p>
		<%= f.label :title %>
		<%= f.text_field :title %>
	</p>
	<p>
		<%= f.label :description %>
		<%= f.text_field :description %>
	</p>
	<p>
		<%= f.label :price_per_foot %>
		<%= f.text_field :price_per_foot %>
	</p>
	<%= f.submit %>
<% end %>
